<div id="modalIndicarPresenca" class="modal fade" role="dialog">
    <div class="modal-dialog">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Indicar presença</h4>
            </div>
            <div class="modal-body modal-lg">

                <div class="container-fluid">
                    <div class="row">

                        <div class="form-inline">

                            <input type="hidden" name="IdAgendaPresenca" id="IdAgendaPresenca" value="0" />

                            <div class="form-group col-xs-12 col-sm-12 col-md-12">
                                <label class="control-label">Profissional</label>
                                <p class="" id="ProfissionalPresenca"></p>
                            </div>

                            <div class="form-group col-xs-12 col-sm-12 col-md-12">
                                <label class="control-label">Especialidade</label>
                                <p class="" id="EspecialidadePresenca"></p>
                            </div>

                            <div class="form-group col-xs-6 col-sm-6 col-md-6">
                                <label class="control-label">Data/Hora Início</label>
                                <p class="" id="InicioConsultaPresenca"></p>
                            </div>

                            <div class="form-group col-xs-6 col-sm-6 col-md-6">
                                <label class="control-label">Data/Hora Fim</label>
                                <p class="" id="FimConsultaPresenca"></p>
                            </div>

                            <div class="form-group col-xs-12 col-sm-12 col-md-12">
                                <label class="control-label label-obrigatorio">Situação</label>
                                <label class="radio-inline">
                                    <input type="radio" name="SituacaoPresenca"  value="1" checked>Presente
                                </label>
                                <label class="radio-inline">
                                    <input type="radio" name="SituacaoPresenca" value="2">Ausente
                                </label>
                            </div>

                            <div class="form-group col-xs-12 col-sm-12 col-md-12">
                                <label class="control-label">Justificativa</label>
                                <textarea name="Justificativa" class="form-control" rows="5"></textarea>
                            </div>

                        </div>

                    </div>

                </div>

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-success" onclick="modalPresenca.salvarPresenca()">Salvar</button>
                <button type="button" class="btn btn-default" data-dismiss="modal">Fechar</button>
            </div>
        </div>

    </div>
</div>
