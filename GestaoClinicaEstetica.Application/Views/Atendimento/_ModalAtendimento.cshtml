<div id="modalAtendimento" class="modal fade" role="dialog">
    <div class="modal-dialog">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Atendimento</h4>
            </div>
            <div class="modal-body modal-lg">

                <div class="container-fluid">
                    <div class="row">

                        <div class="form-inline">

                            <input type="hidden" name="IdAgendaAtendimento" id="IdAgendaAtendimento" value="0" />

                            <div class="form-group col-xs-12 col-sm-12 col-md-12">
                                <label class="control-label">Profissional</label>
                                <p class="" id="ProfissionalAtendimento"></p>
                            </div>

                            <div class="form-group col-xs-12 col-sm-12 col-md-12">
                                <label class="control-label">Especialidade</label>
                                <p class="" id="EspecialidadeAtendimento"></p>
                            </div>

                            <div class="form-group col-xs-6 col-sm-6 col-md-6">
                                <label class="control-label">Data/Hora Início</label>
                                <p class="" id="InicioConsultaAtendimento"></p>
                            </div>

                            <div class="form-group col-xs-6 col-sm-6 col-md-6">
                                <label class="control-label">Data/Hora Fim</label>
                                <p class="" id="FimConsultaAtendimento"></p>
                            </div>

                            <div class="form-group col-xs-12 col-sm-12 col-md-12">
                                <label class="control-label">Atendimento</label>
                                <textarea name="ObsAtendimento" class="form-control" rows="5"></textarea>
                            </div>

                        </div>

                    </div>

                </div>

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-success" onclick="modalAtendimento.salvarAtendimento()">Salvar</button>
                <button type="button" class="btn btn-default" data-dismiss="modal">Fechar</button>
            </div>
        </div>

    </div>
</div>
